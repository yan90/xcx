.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 详情页的小图标 */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_2216517_9i44db1vcz5.eot?t=1605838132320'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2216517_9i44db1vcz5.eot?t=1605838132320#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_2216517_9i44db1vcz5.woff?t=1605838132320') format('woff'),
  url('//at.alicdn.com/t/font_2216517_9i44db1vcz5.ttf?t=1605838132320') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
